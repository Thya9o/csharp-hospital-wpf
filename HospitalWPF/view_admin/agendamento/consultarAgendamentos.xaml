<Window x:Class="HospitalWPF.view_admin.agendamento.consultarAgendamentos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalWPF.view_admin.agendamento"
        mc:Ignorable="d"
        Title="Agendamentos" Height="300" Width="534">
    <Grid>
        <DataGrid x:Name="gridObjetos" ItemsSource="{Binding Path=Objetos}" SelectedItem="{Binding Path=Objeto, Mode=TwoWay}" Margin="20,0,20,80"/>
        <Button x:Name="btnCadastrar" Content="Cadastrar" HorizontalAlignment="Left" Margin="20,0,0,20" VerticalAlignment="Bottom" Width="120" Height="50" Click="btnCadastrar_Click"/>
        <Button x:Name="btnAtualizar" Content="Atualizar" HorizontalAlignment="Left" Margin="160,0,0,20" VerticalAlignment="Bottom" Width="120" Height="50" Click="btnAtualizar_Click"/>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Right" Margin="0,0,20,20" VerticalAlignment="Bottom" Width="120" Height="50" Click="btnVoltar_Click"/>
    </Grid>
</Window>
