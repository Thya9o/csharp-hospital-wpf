<Window x:Class="HospitalWPF.view_admin.areaAtuacao.consultarAreaAtuacao"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalWPF.view_admin.areaAtuacao"
        mc:Ignorable="d"
        Title="Consultar Áreas de Atuação" Height="340" Width="630">
    <Grid>
        <DataGrid x:Name="gridObjetos" ItemsSource="{Binding Objetos}" SelectedItem="{Binding Path=Objeto, Mode=TwoWay}" Margin="17,0,24,80"/>
        <Button x:Name="btnCadastrar" Content="Cadastrar" HorizontalAlignment="Left" Margin="17,235,0,0" VerticalAlignment="Top" Width="102" Height="50" Click="btnCadastrar_Click"/>
        <Button x:Name="btnAtualizar" Content="Atualizar" HorizontalAlignment="Left" Margin="136,235,0,0" VerticalAlignment="Top" Width="103" Height="50" Click="btnAtualizar_Click"/>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Right" Margin="0,235,24,0" VerticalAlignment="Top" Width="102" Height="50" Click="btnVoltar_Click"/>
    </Grid>
</Window>
